<UserControl x:Class="DestinyNetViews.YearView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:DestinyNet="clr-namespace:DestinyNet"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:YearView="clr-namespace:UIYearControl;assembly=YearControl"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=DestinyNet:YearViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <YearView:YearControl Background="White" Margin="20,20" Date="{Binding Path=Date}"> </YearView:YearControl>
    </Grid>
</UserControl>
