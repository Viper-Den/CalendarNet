<UserControl x:Class="DestinyNetViews.YearView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:DestinyNet="clr-namespace:DestinyNet"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:YearView="clr-namespace:UIYearControl;assembly=YearControl"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=DestinyNet:YearViewModel}"
             d:DesignHeight="700" d:DesignWidth="1000">
    <Grid >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="109*"/>
            <ColumnDefinition Width="16*"/>
        </Grid.ColumnDefinitions>
        <YearView:YearControl Background="White" Margin="20,20,20,20" Date="{Binding Path=Date}" DateRanges="{Binding Path=DateRanges}" Grid.ColumnSpan="2"></YearView:YearControl>
    </Grid>
</UserControl>
