<Page x:Class="DestinyNet.PageYear"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DestinyNet"
      xmlns:YearView="clr-namespace:UIYearControl;assembly=UIYearControl"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageYear">

    <YearView:YearControl Margin="20,20,20,20" 
            Date="{Binding Path=DateYear}"  DateRanges="{Binding Path=DateRanges}">
    </YearView:YearControl>
</Page>
